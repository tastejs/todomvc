        "src/js/views/todo-item.js",
        "src/js/views/stats.js",
        "src/js/views/app.js",
        "src/js/routers/todomvc.js",
        "src/js/app.js"
      ]
    }
  },
  "templates": {
    "template": "Thorax.templates['{{{without-extension name}}}'] = '{{{data}}}';",
    "src/js/views/app.js": [
      "src/templates/app.handlebars"
    ],
    "src/js/views/stats.js": [
      "src/templates/stats.handlebars"
    ]
